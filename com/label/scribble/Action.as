package com.label.scribble{	import com.label.timesquid.ILimb;	import flash.events.Event;	import flash.events.EventDispatcher;	public class Action extends EventDispatcher implements com.label.timesquid.ILimb {				public var theVal;		public var rawVal;				public var arr:Array = new Array();		protected var smooth:int = 10;				function Action()		{			trace("New Action");		}				public function setValue(val:int)		{			theVal = val;			rawVal = val;			arr.push(val);			if (arr.length>smooth)			{				arr = arr.slice(arr.length-smooth, smooth);			}			var sum:int=0;			for (var i:int = 0; i<arr.length; i++)			{				sum+=arr[i];			}			sum = sum/arr.length;			theVal = sum;			//trace("SUM: ", sum, val, arr.length);			var e:Event = new Event("Scribble");				dispatchEvent(e);			if (val<350)			{				//trace("Action val:", val);											}		}	}}