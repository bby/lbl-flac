package com.label.timesquid.bill {		import flash.display.MovieClip;	import com.label.timesquid.*;			public class Hand extends MovieClip implements ILimb {				// Constants:		// Public Properties:		// Private Properties:		var min = 170;		var max = 290;		// UI Elements:				public function Hand()		{			//this.rotation = 120;			this.stop();			this.scaleY = 1;		}				public function setValue(val:int) 		{						//Calc range			trace("val", val);			var range = (340); //340-590			var newVal = (val-range);			//trace("range", range, "new Val", newVal, "calc: ", (newVal/10), val);			this.gotoAndStop(Math.floor(20-(newVal/10)));			//trace("this x: ", this.scaleX, this.scaleY);							}				public function isClosed():Boolean		{			return this.currentFrame>=15;					}	}	}